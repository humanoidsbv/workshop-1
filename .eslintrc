{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": ["error", "never"],
    "import/no-extraneous-dependencies": ['error', { devDependencies: [
      "*.js",
      "src/*.js",
      "src/test/**/*.js",
    ] }],
  }
}
